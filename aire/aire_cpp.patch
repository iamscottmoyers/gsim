+void TemplatedListTest::append_element_01()
+{
+	int i;
+	IIR_StatementList *list = new IIR_StatementList();
+	IIR_Statement *statements[TEST_SIZE];
+	IIR_Statement *iter;
+
+	CPPUNIT_ASSERT_EQUAL_MESSAGE( "Incorrect number of elements.", 0, list->number_of_elements() );
+	for( i = 0; i < TEST_SIZE; i++ )
+	{
+		statements[i] = new IIR_Statement();
+		list->append_element(statements[i]);
+	}
+
+	/* Check number of elements */
+	CPPUNIT_ASSERT_EQUAL_MESSAGE( "Incorrect number of elements.", TEST_SIZE, list->number_of_elements() );
+
+	/* Check forward iteration */
+	i = 0;
+	for(iter = list->get_first_element(); iter; iter = list->get_successor_element(iter) )
+	{
+		CPPUNIT_ASSERT_EQUAL_MESSAGE( "Incorrect value.", statements[i], iter );
+		CPPUNIT_ASSERT_EQUAL_MESSAGE( "Iterator nth doesn't match get_nth_element().",
+		                              list->get_nth_element(i), iter );
+		i++;
+	}
+
+	CPPUNIT_ASSERT_EQUAL_MESSAGE( "Iterator did not complete.", i, list->number_of_elements() );
+
+	/* Check backward iteration */
+	i = list->number_of_elements() - 1;
+	for( iter = list->get_last_element(); iter; iter = list->get_predecessor_element(iter) )
+	{
+		CPPUNIT_ASSERT_EQUAL_MESSAGE( "Incorrect statement.", statements[i], iter );
+		CPPUNIT_ASSERT_EQUAL_MESSAGE( "Iterator nth doesn't match get_nth_element().",
+		                              list->get_nth_element(i), iter );
+		i--;
+	}
+
+	CPPUNIT_ASSERT_EQUAL_MESSAGE( "Iterator did not complete.", -1, i );
+
+	delete list;
+}
