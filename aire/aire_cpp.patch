+#ifndef _AIRE_TEMPLATEDLIST_H_
+#define _AIRE_TEMPLATEDLIST_H_
+
+#include <cstring>
+#include <cassert>
+
+#include "primitive_types.h"
+#include "IIR_List.h"
+
+template <class T>
+class _TemplatedList : public IIR_List
+{
+private:
+	T *_front;
+	T *_back;
+public:
+	_TemplatedList(void);
+	~_TemplatedList(void);
+	void prepend_element(T *element);
+	void append_element(T *element);
+	IR_Boolean insert_after_element(T *existing_element, T *new_element);
+	IR_Boolean insert_before_element(T *existing_element, T *new_element);
+	IR_Boolean remove_element(T *existing_element);
+	T *get_successor_element(T *existing_element);
+	T *get_predecessor_element(T *existing_element);
+	T *get_first_element(void);
+	T *get_nth_element(IR_Int32 index);
+	T *get_last_element(void);
+	IR_Int32 get_element_position(T *existing_element);
+};
+
+template<class T> _TemplatedList<T>::_TemplatedList(void)
+{
+	this->_front = NULL;
+	this->_back = NULL;
+}
+
+template<class T> _TemplatedList<T>::~_TemplatedList(void)
+{
+	T *el = this->get_first_element( );
+	while( el )
+	{
+		T *old_el = el;
+		el = this->get_successor_element( el );
+		delete old_el;
+	}
+}
+
+template<class T> void _TemplatedList<T>::prepend_element(T *element)
+{
+	T *next;
+
+	if( element == NULL ) return;
+
+	assert( element->T::_link.list == NULL && "element is already part of another list of this type.");
+
+	next = this->_front;
+	this->_front = element;
+	element->T::_link.list = this;
+	element->T::_link.prev = NULL;
+	element->T::_link.next = next;
+
+	if( next != NULL )
+	{
+		next->T::_link.prev = element;
+	}
+	else
+	{
+		this->_back = element;
+	}
+
+	this->_elements++;
+}
+
+template<class T> void _TemplatedList<T>::append_element(T *element)
+{
+	T *prev;
+
+	if( element == NULL ) return;
+
+	assert( element->T::_link.list == NULL && "element is already part of another list of this type.");
+
+	prev = this->_back;
+	this->_back = element;
+	element->T::_link.list = this;
+	element->T::_link.next = NULL;
+	element->T::_link.prev = prev;
+
+	if( prev != NULL )
+	{
+		prev->T::_link.next = element;
+	}
+	else
+	{
+		this->_front = element;
+	}
+
+	this->_elements++;
+}
+
+template<class T> IR_Boolean _TemplatedList<T>::insert_after_element(T *existing_element, T *new_element)
+{
+	if( new_element == NULL ) return true;
+	if( existing_element->T::_link.list != this ) return false;
+
+	new_element->T::_link.list = this;
+
+	new_element->T::_link.next = existing_element->T::_link.next;
+	if( existing_element->T::_link.next != NULL )
+	{
+		T *next = existing_element->T::_link.next;
+		next->T::_link.prev = new_element;
+	}
+
+	new_element->T::_link.prev = existing_element;
+	existing_element->T::_link.next = new_element;
+
+	if( existing_element == this->_back )
+	{
+		this->_back = new_element;
+	}
+
+	this->_elements++;
+
+	return true;
+}
+
+template<class T> IR_Boolean _TemplatedList<T>::insert_before_element(T *existing_element, T *new_element)
+{
+	if( new_element == NULL ) return true;
+	if( existing_element->T::_link.list != this ) return false;
+
+	new_element->T::_link.list = this;
+
+	new_element->T::_link.prev = existing_element->T::_link.prev;
+	if( existing_element->T::_link.prev != NULL )
+	{
+		T *prev = existing_element->T::_link.prev;
+		prev->T::_link.next = new_element;
+	}
+
+	new_element->T::_link.next = existing_element;
+	existing_element->T::_link.prev = new_element;
+
+	if( existing_element == this->_front )
+	{
+		this->_front = new_element;
+	}
+
+	this->_elements++;
+
+	return true;
+}
+
+template<class T> IR_Boolean _TemplatedList<T>::remove_element(T *existing_element)
+{
+	if( existing_element == NULL || existing_element->T::_link.list != this ) return false;
+
+	if( existing_element == this->_front )
+	{
+		this->_front = existing_element->T::_link.next;
+	}
+
+	if( existing_element == this->_back )
+	{
+		this->_back = existing_element->T::_link.prev;
+	}
+
+	if( existing_element->T::_link.prev != NULL )
+	{
+		T *prev = existing_element->T::_link.prev;
+		prev->T::_link.next = existing_element->T::_link.next;
+	}
+
+	if( existing_element->T::_link.next != NULL )
+	{
+		T *next = existing_element->T::_link.next;
+		next->T::_link.prev = existing_element->T::_link.prev;
+	}
+
+	existing_element->T::_link.list = NULL;
+	this->_elements--;
+
+	return true;
+}
+
+template<class T> T *_TemplatedList<T>::get_successor_element(T *existing_element)
+{
+	if( existing_element == NULL || existing_element->T::_link.list != this ) return NULL;
+
+	return existing_element->T::_link.next;
+}
+
+template<class T> T *_TemplatedList<T>::get_predecessor_element(T *existing_element)
+{
+	if( existing_element == NULL || existing_element->T::_link.list != this ) return NULL;
+
+	return existing_element->T::_link.prev;
+}
+
+template<class T> T * _TemplatedList<T>::get_first_element(void)
+{
+	return this->_front;
+}
+
+template<class T> T *_TemplatedList<T>::get_nth_element(IR_Int32 index)
+{
+	T *el;
+
+	if( index >= this->_elements ) return NULL;
+
+	if( index < (this->_elements / 2) )
+	{
+		el = this->get_first_element( );
+		while( index-- )
+		{
+			el = this->get_successor_element( el );
+		}
+	}
+	else
+	{
+		el = this->get_last_element( );
+		index = this->_elements - index - 1;
+		while( index-- )
+		{
+			el = this->get_predecessor_element( el );
+		}
+	}
+
+	return el;
+}
+
+template<class T> T *_TemplatedList<T>::get_last_element(void)
+{
+	return this->_back;
+}
+
+template<class T> IR_Int32 _TemplatedList<T>::get_element_position(T *existing_element)
+{
+	IR_Int32 pos = 0;
+	T *el;
+
+	if( existing_element == NULL || existing_element->T::_link.list != this ) return -1;
+
+	el = this->get_first_element( );
+	assert( el != NULL && "Can't find existing_element in the list it claims to be in." );
+	while( el != existing_element )
+	{
+		pos++;
+		el = this->get_successor_element( el );
+		assert( el != NULL && "Can't find existing_element in the list it claims to be in." );
+	}
+	return pos;
+}
+
+#endif /*!_AIRE_TEMPLATEDLIST_H_*/
